n=100000

echo "entro al bucle"
while [ "$n" != "0" ]
do
curl -H "Content-Type: application/json" -d '{"GCIDCallId": "'$RANDOM'", "CallLegId": "'$RANDOM'", "calling": "'$RANDOM'", "called": "'$RANDOM'", "date": "2015-03-24", "time": "16:12:54", "duration": "'$RANDOM'", "path": "2015/03/24/16/3505_3103_27_47582016_20150324-161254.mp3"}' 127.0.0.1:3000/api/calls                                                                                           
echo $n
#sleep $((RANDOM % 3))                                                                             
n=`expr $n - 1`
done
echo "salgo del bucle"
