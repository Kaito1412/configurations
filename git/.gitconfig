[user]
  name = Kaito Mateu
  email = kaito@cryptolab.net
[core]
  editor = vim
[alias]
  a = add
  br = branch
  c = commit
  ca = commit --amend
  cm = commit -m
  cl = clean
  co = checkout
  cp = cherry-pick
  dt = difftool
  ft = fetch
  l = log
  la = log --all
  lp = log --graph --decorate --format=format:'%C(bold blue)%H%C(reset) - %C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
  mg  = merge
  mt = mergetool
  r = reset
  rb = rebase
  rf = reflog
  rh = reset --hard
  rs = reset --soft
  rv = revert
  s = stash
  sa = stash apply stash
  sl = stash list
  st = status
[color]
  ui = auto
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[merge]
  tool = vimdiff
[mergetool]
  keepBackup = false
[push]
  default = current
